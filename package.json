{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "build-debug": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug",
    "build-release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleRelease",
    "lint:rules": "eslint . --cache",
    "lint:code-format": "prettier --check \"{src,__mocks__}/**/*.{js,json,md,ts,tsx,yml,yaml}\"",
    "lint:type-check": "tsc",
    "lint": "yarn lint:rules && yarn lint:code-format && yarn lint:type-check",
    "lint:fix": "yarn lint:rules --fix && yarn lint:code-format --write && yarn lint:type-check",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "pod-install": "npx pod-install",
    "postinstall": "npx patch-package",
    "link-custom-fonts": "npx react-native-asset"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-native-vector-icons/common": "^11.0.0",
    "@react-native-vector-icons/feather": "^4.29.2",
    "@react-native-vector-icons/material-icons": "^0.0.1",
    "@react-native-vector-icons/simple-line-icons": "^2.5.5",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.2.10",
    "@ronradtke/react-native-markdown-display": "^8.1.0",
    "@tanstack/react-query": "^5.59.8",
    "@types/markdown-it": "^14.1.2",
    "i18next": "^25.0.2",
    "intl-pluralrules": "^2.0.1",
    "jotai": "^2.11.0",
    "jotai-effect": "^2.0.2",
    "ky": "^1.7.2",
    "markdown-it": "^14.1.0",
    "react": "19.1.0",
    "react-error-boundary": "^5.0.0",
    "react-i18next": "^15.0.2",
    "react-native": "^0.79.1",
    "react-native-external-keyboard": "^0.5.4",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-mmkv": "^3.1.0",
    "react-native-reanimated": "^3.16.7",
    "react-native-reorderable-list": "^0.14.0",
    "react-native-saf-x": "^2.2.3",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "4.10.0",
    "react-native-svg": "^15.11.1",
    "react-native-toast-message": "^2.2.1",
    "react-native-uuid": "^2.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.1",
    "@react-native/eslint-config": "0.79.1",
    "@react-native/metro-config": "0.79.1",
    "@react-native/typescript-config": "0.79.1",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-test-renderer": "^19.1.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-root-import": "^6.6.0",
    "dotenv": "^16.3.1",
    "eslint": "^9.25.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-perfectionist": "^4.12.3",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-unicorn": "^59.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "19.1.0",
    "reactotron-react-native": "^5.1.9",
    "reactotron-react-native-mmkv": "^0.2.7",
    "reactotron-react-query": "^1.0.4",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "engines": {
    "node": ">=18"
  }
}
